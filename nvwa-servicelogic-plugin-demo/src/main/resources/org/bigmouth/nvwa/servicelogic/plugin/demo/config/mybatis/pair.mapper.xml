<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bigmouth.nvwa.servicelogic.plugin.demo.pair.dao.PairDao">
	<resultMap type="org.bigmouth.nvwa.servicelogic.plugin.demo.pair.Pair" id="Pair">
		<id column="id" property="id"/>
		<result column="key" property="key"/>
		<result column="value" property="value"/>
		<result column="des" property="desc"/>
	</resultMap>
	
	<select id="queryAll" resultMap="Pair">
		SELECT key, value, des, client_use FROM tbl_fm_base_conf_pair WHERE deleted = 0
	</select>
</mapper>